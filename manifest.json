{
  "author": "Shubham Verma",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "./assets/code-insiders-19.png",
    "default_title": "Code-to-Code-Insiders Popup"
  },
  "content_scripts": [
    {
      "js": ["content_script.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "description": "Lets you open VSCode links into VSCode Insiders",
  "icons": {
    "16": "./assets/logo-16.png",
    "48": "./assets/logo-48.png",
    "128": "./assets/logo-128.png"
  },
  "manifest_version": 2,
  "name": "Code-to-Code-Insiders",
  "version": "1.0.0"
}
